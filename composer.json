{
    "name": "chronostep/chronoslack",
    "description": "This is slack Logging notification.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chronostep",
            "email": "info@chronostep.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/slack-notification-channel": "^2.3",
        "chronostep/chronobrowserplatform": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Chronostep\\Chronoslack\\": "src/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chronostep\\Chronoslack\\SlackLogServiceProvider",
                "Chronostep\\Chronobrowserplatform\\BPDetectServiceProvider"
            ],
            "aliases": {
                "SlackLog": "Chronostep\\Chronoslack\\Facades\\SlackLog",
                "BPDetect": "Chronostep\\Chronobrowserplatform\\Facades\\BPDetect"
            }
        }
    }
}
